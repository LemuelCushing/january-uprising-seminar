@article{Kutolowski1979,
abstract = {Le r{\^{o}}le de la propagande dans la formation de l{\'{o}}pinion publique est complexe. Cependant les {\'{e}}tudes de l{\'{o}}pinion victorienne en mati{\`{e}}re d{\'{a}}ffaires {\'{e}}trang{\`{e}}res acceptent simplement que les courants historiques de l{\'{e}}́poque— c{\'{e}}st-{\`{a}}-dire les images st{\'{e}}r{\'{e}}otyp{\'{e}}es d{\'{a}}utres nations, les tendances politiques, et les valeurs religieuses et sociales— s{\'{i}}nt{\`{e}}grent avec les {\'{e}}v{\'{e}}nements de l{\'{e}}́tranger pour en faire l{\'{o}}pinion publique. La propagande n{\'{e}}st gu{\`{e}}re trait{\'{e}}e. Alors se r{\'{e}}f{\'{e}}rant {\`{a}} la m{\'{e}}thodologie et aux perspectives des sciences sociales, on peut {\'{e}}claircir ce processus. Au d{\'{e}}but des ann{\'{e}}es 1860 les {\'{e}}migr{\'{e}}s polonais en Angleterre soutenaient leur cause avec beaucoup d{\'{e}}́nergie dans la presse, au “Parlement,” et parmi les classes ouvri{\`{e}}res. Leur meilleur succ{\`{e}}s {\'{e}}tait dans la presse, surtout parmi les r{\'{e}}dacteurs {\`{a}} penchant Lib{\'{e}}ral. Au "Parlement" ils ont soulev{\'{e}} douze fois en 1861-62 le probl{\`{e}}me polonais, dont quatre fois pour un d{\'{e}}bat {\'{e}}tendu. Une campagne vigoureuse de r{\'{e}}unions publiques dans tout le pays a produit un bon nombre de p{\'{e}}titions au “Parlement,” qui refl{\'{e}}taient dans une certaine mesure l{\'{o}}pinion de la classe ouvri{\`{e}}re. Malgr{\'{e}} ces succ{\`{e}}s, la propagande polonaise a {\'{e}}chou{\'{e}} dans son but principal, celui de gagner une approbation g{\'{e}}n{\'{e}}rale pour une initiative diplomatique de la part de l{\'{A}}ngleterre envers la Russie. La propagande polonaise, quoiqu{\'{e}}lle employait des techniques astucieuses, souffrait d{\'{i}}nsuffisances. Les polonais ne pouvaient pas gagner {\`{a}} leur cause l{\'{a}}dh{\'{e}}rance du journal le plus influent, notamment le Times de Londres; ils ne pouvaient pas non plus attirer les caciques du “Parlement”; et enfin leurs manifestations d{\'{e}}pendaient largement de David Urquhart, un eccentrique pas tr{\`{e}}s c{\^{o}}t{\'{e}}. Plus important, le but polonais de provoquer une intervention diplomatique de la part de l{\'{A}}ngleterre ne pouvait pas {\^{e}}tre pr{\'{e}}sent{\'{e}} comme politique pratique. En g{\'{e}}n{\'{e}}ral, trois {\'{e}}l{\'{e}}ments contribuaient {\`{a}} la formation de l{\'{o}}pinion publique: la propagande, les {\'{e}}v{\'{e}}nements en Pologne, et les conditions historiques de l{\'{A}}ngleterre du milieu de l{\`{e}}́re victorienne. La propagande polonaise fut la plus efficace pendant que la contestation en Pologne fut la plus faible: elle fut moins important pendant la p{\'{e}}riode de pleine r{\'{e}}volte en 1863. Bref, les {\'{e}}v{\'{e}}nements eux-m{\^{e}}mes en Pologne contribuaient plus que la propagande polonaise {\`{a}} la formation de l{\'{o}}pinion publique britannique, les circonstances anglaises {\'{e}}tant toujour un facteur important. Sans doute, la propagande contribuait quand m{\^{e}}me {\`{a}} l{\'{e}}́tendue et {\`{a}} l{\'{i}}ntensit{\'{e}} du sentiment britannique envers la Pologne.},
author = {Kutolowski, John F},
journal = {Canadian Slavonic Papers / Revue Canadienne des Slavistes},
number = {1},
pages = {45--65},
title = {{Polish Exiles and British Public Opinion: A Case Study of 1861-62}},
url = {http://www.jstor.org/stable/40867415},
volume = {21},
year = {1979}
}
